#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x7ffff58c1000 .scope module, "controlromtest" "controlromtest" 2 1;
 .timescale 0 0;
v0x7ffff58e1590_0 .net "controlLines", 6 0, v0x7ffff58c1370_0;  1 drivers
v0x7ffff58e1650_0 .var "decoderIn", 63 0;
S_0x7ffff58c1180 .scope module, "DUT" "controlrom" 2 6, 3 1 0, S_0x7ffff58c1000;
 .timescale 0 0;
    .port_info 0 /INPUT 64 "decoderIn"
    .port_info 1 /OUTPUT 7 "controlLines"
v0x7ffff58c1370_0 .var "controlLines", 6 0;
v0x7ffff58e1450_0 .net "decoderIn", 63 0, v0x7ffff58e1650_0;  1 drivers
E_0x7ffff58b6820 .event edge, v0x7ffff58e1450_0;
    .scope S_0x7ffff58c1180;
T_0 ;
    %wait E_0x7ffff58b6820;
    %load/vec4 v0x7ffff58e1450_0;
    %dup/vec4;
    %pushi/vec4 2147483648, 0, 33;
    %concati/vec4 0, 0, 31;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 64, 0, 64;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 16384, 0, 64;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1024, 0, 64;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 2147483648, 0, 40;
    %concati/vec4 0, 0, 24;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %pushi/vec4 0, 0, 7;
    %assign/vec4 v0x7ffff58c1370_0, 0;
    %jmp T_0.6;
T_0.0 ;
    %pushi/vec4 113, 1, 7;
    %assign/vec4 v0x7ffff58c1370_0, 0;
    %jmp T_0.6;
T_0.1 ;
    %pushi/vec4 125, 1, 7;
    %assign/vec4 v0x7ffff58c1370_0, 0;
    %jmp T_0.6;
T_0.2 ;
    %pushi/vec4 18, 0, 7;
    %assign/vec4 v0x7ffff58c1370_0, 0;
    %jmp T_0.6;
T_0.3 ;
    %pushi/vec4 99, 96, 7;
    %assign/vec4 v0x7ffff58c1370_0, 0;
    %jmp T_0.6;
T_0.4 ;
    %pushi/vec4 109, 101, 7;
    %assign/vec4 v0x7ffff58c1370_0, 0;
    %jmp T_0.6;
T_0.6 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x7ffff58c1000;
T_1 ;
    %vpi_call 2 9 "$display", "Control Rom test" {0 0 0};
    %vpi_call 2 10 "$display", "ADD Instruction Output" {0 0 0};
    %pushi/vec4 2147483648, 0, 33;
    %concati/vec4 0, 0, 31;
    %assign/vec4 v0x7ffff58e1650_0, 0;
    %delay 9, 0;
    %vpi_call 2 13 "$display", "controlLines output: %b\012", v0x7ffff58e1590_0 {0 0 0};
    %vpi_call 2 15 "$display", "NOR Instruction Output" {0 0 0};
    %pushi/vec4 64, 0, 64;
    %assign/vec4 v0x7ffff58e1650_0, 0;
    %delay 9, 0;
    %vpi_call 2 18 "$display", "controlLines output: %b\012", v0x7ffff58e1590_0 {0 0 0};
    %vpi_call 2 20 "$display", "LW Instruction Output" {0 0 0};
    %pushi/vec4 16384, 0, 64;
    %assign/vec4 v0x7ffff58e1650_0, 0;
    %delay 9, 0;
    %vpi_call 2 23 "$display", "controlLines output: %b\012", v0x7ffff58e1590_0 {0 0 0};
    %vpi_call 2 25 "$display", "SW Instruction Output" {0 0 0};
    %pushi/vec4 1024, 0, 64;
    %assign/vec4 v0x7ffff58e1650_0, 0;
    %delay 9, 0;
    %vpi_call 2 28 "$display", "controlLines output: %b\012", v0x7ffff58e1590_0 {0 0 0};
    %vpi_call 2 30 "$display", "BEQ Instruction Output" {0 0 0};
    %pushi/vec4 2147483648, 0, 40;
    %concati/vec4 0, 0, 24;
    %assign/vec4 v0x7ffff58e1650_0, 0;
    %delay 9, 0;
    %vpi_call 2 33 "$display", "controlLines output: %b", v0x7ffff58e1590_0 {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "compile/controlromtest.v";
    "compile/controlrom.v";
